{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--port=5000",
                "--ssl-keyfile=./certs/key.pem",
                "--ssl-certfile=./certs/cert.pem"
            ],
            "jinja": true,
            "autoStartBrowser": true
        }
    ]
}